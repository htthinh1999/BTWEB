		try {
			con = DriverManager.getConnection(url, username, password);
			stm = con.createStatement();
			String sql = "SELECT * FROM tbl_edict";
			rs = stm.executeQuery(sql);
			
			rs.next();
			if()
			
			con.close();
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}
}
